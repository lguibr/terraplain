{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#example/*": ["example/*"],
      "#src/*": ["src/*"],
    },
    "alwaysStrict": true,
    "charset": "utf8",
    "declaration": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["es2018"],
    "module": "esnext",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "target": "ES2018",
    "incremental": true,
    "outDir": "./built"
  },
  "include": ["**/*.ts", "src/lib/bundler.js"],
  "exclude": ["node_modules", "cdktf.out", "built"]
}
